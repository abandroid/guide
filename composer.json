{
    "name": "endroid/guide",
    "description": "Endroid Guide",
    "keywords": ["endroid", "guide", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/guide",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-gd": "*",
        "symfony/options-resolver": ">=2.7",
        "symfony/cache": ">=2.7",
        "symfony/dom-crawler": "^3.0|^4.0",
        "symfony/css-selector": "^3.0|^4.0"
    },
    "require-dev": {
        "symfony/asset": ">=2.7",
        "symfony/browser-kit": ">=2.7",
        "symfony/finder": ">=2.7",
        "symfony/framework-bundle": ">=2.7",
        "symfony/http-kernel": ">=2.7",
        "symfony/templating": ">=2.7",
        "symfony/twig-bundle": ">=2.7",
        "symfony/yaml": ">=2.7",
        "sensio/framework-extra-bundle": ">=2.7",
        "phpunit/phpunit": ">=5.7"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Guide\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Guide\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
