{
    "name": "endroid/guide",
    "description": "Endroid Guide",
    "keywords": ["endroid", "guide", "bundle", "symfony", "php"],
    "homepage": "https://github.com/endroid/guide",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-gd": "*",
        "symfony/options-resolver": "^3.0|^4.0",
        "symfony/cache": "^3.0|^4.0",
        "symfony/dom-crawler": "^3.0|^4.0",
        "symfony/css-selector": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Guide\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Guide\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
